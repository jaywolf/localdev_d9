{
  "name": "localdev/drupal-installer",
  "description": "Project template for Drupal 9 projects",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.9",
    "drupal/admin_toolbar": "^3.0",
    "drupal/better_exposed_filters": "^5.0@beta",
    "drupal/core-composer-scaffold": "^9.1",
    "drupal/core-project-message": "^9.1",
    "drupal/core-recommended": "^9.1",
    "drupal/default_content": "^2.0@alpha",
    "drupal/devel": "^2.1",
    "drupal/layout_builder_restrictions": "^2.8",
    "drupal/layout_library": "^1.0@beta",
    "drupal/menu_link_attributes": "^1.2",
    "drupal/metatag": "^1.16",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/redirect": "^1.6",
    "drupal/simple_sitemap": "^3.10",
    "drupal/stage_file_proxy": "^1.1",
    "drupal/svg_formatter": "^1.14",
    "drupal/svg_image": "^1.14",
    "drupal/twig_tools": "^1.3",
    "drupal/twig_tweak": "^3.1",
    "drupal/webform": "^6.0",
    "drupal/webp": "^1.0@beta",
    "drush/drush": "^10.5",
    "wikimedia/composer-merge-plugin": "^2.0"
  },
  "require-dev": {
    "drupal/core-dev": "^9.1"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      }
    },
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
      "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
      "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "drupal-core-project-message": {
      "include-keys": ["homepage", "support"],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                                                         </>",
        "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
        "<bg=blue;fg=white>                                                         </>"
      ]
    }
  }
}
